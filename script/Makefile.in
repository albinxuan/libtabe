@SET_MAKE@

all:
	(cd src; $(MAKE))

.PHONY: clean distclean install
clean:
	(cd src; $(MAKE) clean)

distclean:
	(cd src; $(MAKE) distclean)
	rm -f Makefile config.h config.cache config.log config.status

install:
	(cd src; $(MAKE) install)
