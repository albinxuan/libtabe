LIBTOOL_DEPS = @LIBTOOL_DEPS@
@SET_MAKE@

all:
	(cd src; $(MAKE))

libtool: $(LIBTOOL_DEPS)
	$(SHELL) ./config.status --recheck

.PHONY: clean distclean install
clean:
	(cd src; $(MAKE) clean)

distclean:
	(cd src; $(MAKE) distclean)
	rm -f Makefile config.h config.cache config.log config.status libtool

install:
	(cd src; $(MAKE) install)
